<h1>Medics#index</h1>
<h2>Medicos <%= link_to 'Criar', new_admin_path, class:"btn btn-success pull-right" %></h2>
<br>
<table class="table table-hover">
	<tr>
		<th>Nome</th>
		<th>Email</th>
		<th>Ações</th>
		<th></th>
	</tr>
	<% @admins.each do |admin| %>
	<tr>
		<th><%= admin.name %></th>
		<th><%= admin.user.email %></th>
		<th><%= link_to 'Editar', edit_admin_path(admin) %></th>
		<th><%= link_to 'Excluir', admin_path(admin), method: :delete, data: { confirm: 'Are you sure?' } %></th>
	</tr>
	<% end %>
</table>

